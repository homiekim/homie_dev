{
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "printWidth": 110,
  "semi": false,
  "tabWidth": 2,
  "jsxSingleQuote": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
